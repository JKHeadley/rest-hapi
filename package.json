{
  "name": "rest-hapi",
  "version": "1.3.3",
  "description": "A RESTful API generator for hapi",
  "main": "rest-hapi.js",
  "bin": {
    "rest-hapi-cli": "./rest-hapi-cli.js"
  },
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=4.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "nyc tape ./tests/unit/*.tests.js ./tests/e2e/*.tests.js",
    "posttest": "npm run report-coverage",
    "test-unit": "nyc tape ./tests/unit/*.tests.js",
    "test-e2e": "nyc tape ./tests/e2e/*.tests.js",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "patch-release-git": "git add . && git commit -a -m 'patch release' && git push && npm version patch && npm publish",
    "patch-release": "npm version patch && npm publish",
    "precommit": "lint-staged",
    "lint": "eslint --fix **/*.js ./"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JKHeadley/rest-hapi.git"
  },
  "keywords": [
    "hapi",
    "API",
    "RESTful",
    "mongoose",
    "generator"
  ],
  "author": {
    "name": "Justin Headley",
    "email": "headley.justin@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JKHeadley/rest-hapi/issues/new",
    "email": "headley.justin@gmail.com"
  },
  "homepage": "https://github.com/JKHeadley/rest-hapi#readme",
  "dependencies": {
    "blue-tape": "^1.0.0",
    "boom": "^7.2.0",
    "chalk": "*",
    "extend": "^3.0.2",
    "fs-extra": "^7.0.0",
    "hapi": "^17.5.4",
    "hapi-swagger": "9.1.1",
    "hoek": "^5.0.4",
    "inert": "^5.1.0",
    "joi": "^13.6.0",
    "joi-objectid": "^2.0.0",
    "lodash": "~4.17.11",
    "loggin": "^3.0.1",
    "mkdirp": "0.5.1",
    "mongoose": "^5.2.17",
    "mrhorse": "^3.0.1",
    "query-string": "^6.1.0",
    "require-all": "^3.0.0",
    "tape": "latest",
    "vision": "^5.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "clear-require": "^2.0.0",
    "decache": "4.4.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.0.0-rc.15",
    "lint-staged": "^7.3.0",
    "mockgoose": "^7.3.5",
    "nyc": "^13.0.1",
    "prettier": "1.14.3",
    "proxyquire": "^2.1.0",
    "q": "^1.5.1",
    "rewire": "^4.0.1",
    "rmdir": "^1.2.0",
    "sinon": "^6.3.4",
    "sinon-test": "^2.3.0"
  }
}
